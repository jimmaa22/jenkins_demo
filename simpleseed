for (i = 1; i <=10; ++1) {
job("Job-$1")
{
  parameters {
    stringParam('Object','Undefined','This parameter describes the type of Object')
  }
  steps {
    shell('echo $JOB_NAME $BUILD_NUMBER')
    shell('echo $Object')
  }
  publishers {
       downstream('simpleJobWithParameter','SUCCESS')
                  }
  }
}
